{
  "title": "Dependency Install Duration",
  "type": "timeseries",
  "fieldConfig": {
    "defaults": {
      "unit": "s",
      "thresholds": {
        "mode": "absolute",
        "steps": [
          { "color": "green", "value": null },
          { "color": "orange", "value": 90 },
          { "color": "red", "value": 120 }
        ]
      }
    },
    "overrides": []
  },
  "targets": [
    {
      "refId": "A",
      "datasource": "Prometheus",
      "expr": "dependency_install_duration_seconds",
      "legendFormat": "{{job}}",
      "interval": "5m"
    }
  ],
  "options": {
    "tooltip": {
      "mode": "multi",
      "sort": "none"
    },
    "legend": {
      "displayMode": "list",
      "placement": "bottom"
    }
  },
  "description": "Tracks uv lock sync duration; alerts at 120s threshold"
}
