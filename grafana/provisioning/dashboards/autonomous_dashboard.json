{
  "id": null,
  "title": "SomaBrain Autonomous Operations",
  "tags": ["autonomous", "ops"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Autonomous Actions Success Rate",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "autonomous_actions_success_rate",
          "format": "time_series",
          "legendFormat": "success_rate",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Component Health Status",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "autonomous_component_health_status",
          "legendFormat": "{{component}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Recent Autonomous Actions",
      "gridPos": {"x": 0, "y": 6, "w": 24, "h": 6},
      "targets": [
        {
          "expr": "autonomous_action_events_total",
          "refId": "A",
          "format": "table"
        }
      ]
    }
  ],
  "refresh": "10s"
}
